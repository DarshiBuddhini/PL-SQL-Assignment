CREATE TABLE darshi_branch_tab(
   branch_id VARCHAR2(30),
   branch_name VARCHAR2(200) NOT NULL,
   branch_address VARCHAR2(1000) NOT NULL,
   branch_manager VARCHAR2(200),
   total_war_capacity number(5),
   company_id VARCHAR2(30), 
   PRIMARY KEY(branch_id,company_id)
   
);
CREATE SEQUENCE S_BRANCH_ID_SEQ
       START WITH 1
       INCREMENT BY 1
       NOCACHE
       NOCYCLE;